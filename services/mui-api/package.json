{
  "name": "@mui/api",
  "version": "0.0.1",
  "main": "dist/index.js",
  "private": true,
  "engines": {
    "node": "=18.12.1"
  },
  "scripts": {
    "precommit": "gts lint",
    "prestart": "npm run build",
    "start": "PORT=3001 NODE_ENV=development CORS=local ts-node-dev -r dotenv/config ./src/index.ts",
    "start:local": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile ./src/configs.ts migrate:latest && PORT=3001 NODE_ENV=development CORS=local ts-node-dev -r dotenv/config ./src/index.ts",
    "start:production": "NODE_ENV=production node -r dotenv/config ./dist/src/index.js",
    "test": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "build": "rm -rf ./dist && ./node_modules/.bin/tsc",
    "lint": "gts lint --yarn",
    "clean": "gts clean --yarn",
    "compile": "tsc",
    "fix": "gts fix --yarn",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "modules:backfill": "ts-node-dev -r dotenv/config ./src/scripts/templates/backfillPreviewImages.ts",
    "knex:seed:init": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile ./dist/src/configs.js seed:run --specific=initDB.js",
    "knex:migrate:make": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile ./src/configs.ts migrate:make",
    "knex:migrate:latest": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile ./src/configs.ts migrate:latest",
    "knex:migrate:latest:prod": "knex --knexfile ./dist/src/configs.js migrate:latest",
    "knex:migrate:rollback": "node -r dotenv/config ./node_modules/knex/bin/cli.js  --knexfile ./src/configs.ts migrate:rollback",
    "knex:migrate:up": "node -r dotenv/config ./node_modules/knex/bin/cli.js  --knexfile ./src/configs.ts migrate:up",
    "knex:migrate:down": "node -r dotenv/config ./node_modules/knex/bin/cli.js  --knexfile ./src/configs.ts migrate:down",
    "knex:seed:make": "node -r dotenv/config ./node_modules/knex/bin/cli.js  --knexfile ./src/configs.ts seed:make",
    "knex:seed:run": "node -r dotenv/config ./node_modules/knex/bin/cli.js  --knexfile ./src/configs.ts seed:run"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@types/readable-stream": "^2.3.13",
    "@types/request": "^2.48.8",
    "@types/response-time": "2.3.4",
    "apollo-server-errors": "^3.3.1",
    "aws-sdk": "^2.1544.0",
    "compression": "^1.7.4",
    "cookie-parser": "1.4.5",
    "core-js": "3.6.5",
    "cors": "2.8.5",
    "dotenv": "8.0.0",
    "express": "4.18.2",
    "express-http-context": "^1.2.4",
    "express-request-id": "1.4.1",
    "express-winston": "4.0.5",
    "graphql": "^16.6.0",
    "hot-shots": "8.2.0",
    "http-proxy-middleware": "^2.0.6",
    "launchdarkly-node-server-sdk": "5.14.3",
    "mime": "^4.0.1",
    "node-fetch": "2.6.7",
    "response-time": "2.3.2",
    "sharp": "^0.31.0",
    "uuid": "^8.3.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/jest": "24.0.19",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.6.3",
    "@types/uuid": "^8.3.4",
    "gts": "^3.1.0",
    "husky": "4.2.3",
    "jest": "26.6.0",
    "jest-express": "1.12.0",
    "jest-fetch-mock": "3.0.3",
    "pg-diff-cli": "2.0.5",
    "ts-jest": "26.5.6",
    "ts-loader": "6.0.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "1.0.0-pre.43",
    "typescript": "4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "gts lint"
    }
  }
}
